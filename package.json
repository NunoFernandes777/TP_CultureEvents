{
  "name": "my-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:restart": "npx nodemon --watch app --watch lib --watch next.config.* --ext js,jsx,ts,tsx,mjs,cjs,json --exec \"next dev\"",
    "etl:mongo-to-mysql": "node script/mongoToMySQL.js",
    "test": "node --test \"tests/**/*.test.js\"",
    "test:unit": "node --test \"tests/unit/**/*.test.js\"",
    "test:integration": "node --test \"tests/integration/**/*.test.js\"",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "mongodb": "^7.0.0",
    "mysql2": "^3.16.2",
    "next": "16.1.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
